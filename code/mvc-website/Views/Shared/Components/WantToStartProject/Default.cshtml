<section id="bookanappointment" class="section-trigger bg-dark-section background-image nowebpsupport" style="background-image:url('@(Model.PubliclyExposedStringsProvider.ModernImagesUrl)/background-cascadia.webp')" fallback-image="/background-cascadia.jpg">
    <div class="container">
        <div class="row">
